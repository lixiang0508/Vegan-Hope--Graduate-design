<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zlx.gradthesis.demo.Mapper.ProductImgMapper">


    <select id="selectProductImgByProductId" resultType="com.zlx.gradthesis.demo.bean.ProductImg">
        select * from product_img where product_id=#{productId}
    </select>
</mapper>